    "version": "0.2.0",
    "configurations": [
        {
            "name": "Contagent test data",
            "type": "cppdbg",
            "cwd": ".",
            "request": "launch",
            "program": "${workspaceRoot}/build/contagent-bin",
            "linux": {
                "MIMode": "gdb",
                "miDebuggerPath": "/usr/bin/gdb"
            },
            "args": [
                "1",
                "1000",
                "${workspaceRoot}/test-data/1-run/agents_0.json.zst",
                "${workspaceRoot}/test-data/1-run/beliefs_0.json",
                "${workspaceRoot}/test-data/1-run/behaviours.json",
                "${workspaceRoot}/test-data/1-run/output.json.zst",
            ]
        }
    ]
}